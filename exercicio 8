#include <stdio.h>
int main() {
int vetor[12] = {4, -1, 6, -3, 9, -7, 11, -2, 5, -8, 3, -6};

    for (int i = 0; i < 12; i++) {
        if (vetor[i] < 0) {
            vetor[i] = 0;
        }
    }

    for (int i = 0; i < 12; i++) {
        printf("%d ", vetor[i]);
    }
    printf("\n");

    return 0;
}
