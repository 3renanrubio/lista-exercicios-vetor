#include <stdio.h>
int main() {
int tamanho;
    
    printf("Digite o tamanho: ");
    scanf("%d", &tamanho);
    
    int vetor[tamanho];
    
    printf("Digite os elementos:\n");
    for (int i = 0; i < tamanho; i++) {
        scanf("%d", &vetor[i]);
    }
    
    printf("Os elementos sÃ£o:\n");
    for (int i = 0; i < tamanho; i++) {
        printf("%d ", vetor[i]);
    }
    printf("\n");
    
    return 0;
}
