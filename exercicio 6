#include <stdio.h>
int main() {

    int vetor[15] = {10, 11, 12, 30, 25, 99, 67, 88, 22, 77, 7, 66, 33, 4, 55};
    int i;
    int maior, menor;

    maior = menor = vetor[0];

    for (i = 1; i < 15; i++) {
        if (vetor[i] > maior) {
            maior = vetor[i];
        }
        if (vetor[i] < menor) {
            menor = vetor[i];
        }
    }
    
    printf("Maior valor: %d\n", maior);
    printf("Menor valor: %d\n", menor);

    return 0;
}
